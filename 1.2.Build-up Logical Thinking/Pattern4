//{ Driver Code Starts

#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
class Solution {
  public:
    void printTriangle(int n) {
        // code here
         for(int i=1; i < n+1; i++){
                for(int j = 1; j < i+1; j++){
                    cout<<i<<" ";
                }
            cout<<endl;
    }
    }
};
